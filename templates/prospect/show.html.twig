{% extends 'base.html.twig' %}

{% block title %}Prospect{% endblock %}

{% block body %}
    <div id="utilisateur"></div>
    <h1>Prospect</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ prospect.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ prospect.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ prospect.prenom }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ prospect.email  }}</td>
            </tr>
            <tr>
                <th>Societe</th>
                <td>{{ prospect.societe }}</td>
            </tr>
            <tr>
                <th>Téléphone</th>
                <td>{{ prospect.telephone }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ prospect.adresse }}</td>
            </tr>
            <tr>
                <th>Siret</th>
                <td>{{ prospect.siret }}</td>
            </tr>
        </tbody>
    </table>
    <a class="customButton" href="{{ path('prospect_edit', {'id': prospect.id}) }}">Modifier</a>
    <a class="customButton" id="back" href="{{ path('prospect_index') }}">Retour à la liste</a>
    {{ include('prospect/_delete_form.html.twig') }}
{% endblock %}


{% block javascripts %}
    {{ encore_entry_script_tags('rep_log_react') }}
{% endblock %}
